{
  "name": "doctify",
  "displayName": "doctify",
  "description": "Documentation for your function code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "docs.write",
        "category": "Doctify",
        "title": "Generate Docs"
      }
    ]
  },
  "activationEvents": [
  ],
  "keybindings": [
    {
      "command": "docs.write",
      "key": "ctrl+.",
      "mac": "ctrl+.",
      "when": "isWindows && editorTextFocus && editorLangId =~ /typescript|javascript|python|php|java|c|cpp|csharp|dart|ruby|go|rust/ && config.docwriter.hotkey.mac == 'âŒ˜ + .'"
    }],
  "main": "./extension.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.8"
  }
}
